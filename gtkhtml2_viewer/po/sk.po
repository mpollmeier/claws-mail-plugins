#: src/gtkhtml2_viewer.c:882
msgid ""
"Remote images exist, but weren't loaded\n"
"according to your preferences."
msgstr ""
"Vzdialené obrázky existujú, ale podľa vašich nastavení \n"
"neboli načítané."

#: src/gtkhtml2_viewer.c:884
msgid ""
"Remote images exist, but weren't loaded\n"
"because you are offline."
msgstr ""
"Vzdialené obrázky existujú, ale neboli načítané\n"
"pretože ste odpojený."

#: src/gtkhtml2_viewer.c:886
msgid "Remote images exist, but loading them failed."
msgstr "Vzdialené obrázky existujú, ale ich načítanie zlyhalo."

#: src/gtkhtml2_viewer.c:890
msgid "Load images"
msgstr "načítať obrázky"

#: src/gtkhtml2_viewer.c:1126
#, c-format
msgid ""
"Printing failed:\n"
" %s"
msgstr ""
"Tlačenie zlyhalo:\n"
" %s"


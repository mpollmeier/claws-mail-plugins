#: src/gtkhtml2_prefs.c:168
msgid "Cache cleared."
msgstr "A gyorsítótár törölve."

#: src/gtkhtml2_prefs.c:194
msgid "Load remote images in mails"
msgstr "Levelekben lévő távoli képek betöltése"


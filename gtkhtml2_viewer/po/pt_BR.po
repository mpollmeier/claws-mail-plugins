#: src/gtkhtml2_prefs.c:103
msgid "GtkHtml2 Viewer"
msgstr "Visualizador GtkHtml2"

#: src/gtkhtml2_prefs.c:168
msgid "Cache cleared."
msgstr "O cache foi limpo."


msgstr "/Copiar este en_lace"

#: src/gtkhtml2_viewer.c:107
msgid "/Zoom _in"
msgstr "/_Aumentar"

#: src/gtkhtml2_viewer.c:108
msgid "/Zoom _out"
msgstr "/_Disminuir"


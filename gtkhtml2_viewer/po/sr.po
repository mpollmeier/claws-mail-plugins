#: src/gtkhtml2_prefs.c:80
msgid "Plugins"
msgstr "Додаци"

#: src/gtkhtml2_prefs.c:81
msgid "GtkHtml2 Viewer"
msgstr "GtkHtml2 прегледник"

#: src/gtkhtml2_prefs.c:128
msgid "Cache cleared."
msgstr "Кеш је очишћен"

#: src/gtkhtml2_prefs.c:148
msgid "Load remote images in mails"
msgstr "Учитавање удаљених слика у поруку"

#: src/gtkhtml2_prefs.c:155
msgid "Cache remote images locally"
msgstr "Локално кеширање удаљених слика"

#: src/gtkhtml2_prefs.c:162
msgid "Clear images cache when quitting"
msgstr "Брисање кешираних слика при затварању програма"

#: src/gtkhtml2_prefs.c:169
msgid "Clear image cache now"
msgstr "Обриши кеширане слике сада"

#: src/gtkhtml2_viewer.c:532
#, c-format
msgid "Timeout connecting to %s\n"
msgstr "Истекло време за повезивање са %s\n"

#: src/gtkhtml2_viewer.c:600
msgid ""
"Remote images exist, but weren't loaded\n"
"according to your preferences."
msgstr ""
"Постоје удаљене слике, али нису учитане\n"
"по вашим подешавањима."

#: src/gtkhtml2_viewer.c:602
msgid ""
"Remote images exist, but weren't loaded\n"
"because you are offline."
msgstr ""
"Постоје удаљене слике, али нису учитане\n"
"зато што нисте повезани на Интернет."

#: src/gtkhtml2_viewer.c:604
msgid "Remote images exist, but loading them failed."
msgstr "Постоје удаљене слике, али нису успешно учитане."

#: src/gtkhtml2_viewer.c:607
msgid "Load images"
msgstr "Учитај слике"

#: src/gtkhtml2_viewer.c:780
msgid ""
"Your version of Sylpheed-Claws is newer than the version the Gtkhtml2Viewer "
"plugin was built with"
msgstr "Sylpheed-Claws који користите је новиј иод верзије за коју је компајлиран Gtkhtml2Viewer додатак"

#: src/gtkhtml2_viewer.c:785
msgid "Your version of Sylpheed-Claws is too old for the Gtkhtml2Viewer plugin"
msgstr "Sylpheed-Claws који користите је сувише стар за Gtkhtml2Viewer додатак"

#: src/gtkhtml2_viewer.c:822
msgid "GtkHtml2 HTML Viewer"
msgstr "GtkHtml2 HTML прегледник"

#: src/gtkhtml2_viewer.c:827
msgid "This plugin renders HTML mail using the gtkhtml2rendering widget."
msgstr "Овај додатак рендерује HTML поруке користећи gtkhtml2rendering виџет."

#: src/gtkhtml2_viewer.c:849
msgid "text/html"
msgstr "text/html"

